{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Permissions-Policy", "value": "payment=(self \"https://pay.ozow.com\")" },
        { "key": "Content-Security-Policy", "value": "frame-src 'self' https://pay.ozow.com https://static-content.ozow.com;" }
      ]
    }
  ]
}
